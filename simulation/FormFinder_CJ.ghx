class Brickwall(object):
    
    """
    this is a class to create a wall from a input curve and get the origin for its bricks
    
    """
    
    # this varible was defined outside of the class
    # we need to declare it as global so it will be shared by all instances of the class
    global height
    
    
    # this method gets calles as soon as the object of a class is initiated
    # here we define instance variables = attributes of the object
    def __init__(self, crv = None, srf = None , brep = None, layer_cnt = None):
        
        self.layer_cnt = layer_cnt
        self.brick_cnt = None
        
        self.crv = crv
        self.srf = srf
        self.brep = brep
        self.crvs = []
        self.bricks = []
        
        if self.crv is not None:
            self.wall_from_curve(self.crv)
        elif self.srf is not None:
            self.wall_from_surface(srf)
        else:
            self.wall_from_brep(brep)
        
    def get_layers(self, curve):
        
        crvs = []
        for i in range(self.layer_cnt):
            T = rg.Transform.Translation(rg.Vector3d(0,0,i*height))
            c = copy.copy(curve)
            c.Transform(T)
            crvs.append(c)
        return crvs
    
    def get_bricks(self):
        for i, c in enumerate(self.crvs):
            self.brick_cnt = math.floor(c.GetLength()/(length+5))
            params = c.DivideByCount(self.brick_cnt*2,True)
            pts = []
            tngts = []
            # list comprehension
            bricks = [Brick(c.PointAt(p),c.TangentAt(p)) for p in params[i%2::2]]  #Creating objects of type Brick class
            self.bricks.append(bricks)
    
    def create_contours(self, srf, height):
        bounding_box = rg.Brep.GetBoundingBox(srf, rg.Plane.WorldXY)
        num_layers =  int(math.ceil(bounding_box.Max.Z/height))
        inter_crvs = []
        for i in range(num_layers):
            origin = rg.Point3d(0,0,i*height)
            if type(srf) != type(rg.Brep()):
                brep = srf.ToBrep()
            else:
                brep = self.brep
            plane = rg.Plane(origin, rg.Vector3d.XAxis, rg.Vector3d.YAxis)
            rg.Plane.Rotate(plane, angle,rg.Vector3d.XAxis, plane.Origin)
            plane.Transform(rg.Transform.Translation(rg.Vector3d(0,0,-0.1)))
            intersection_event = rg.Intersect.Intersection.BrepPlane(brep,plane,tolerance)
            inter_crvs.append(intersection_event[1][0])
        return inter_crvs
    
    def wall_from_curve(self, curve):
        self.crvs = self.get_layers(curve)
        self.get_bricks()
    
    def wall_from_surface(self,srf):
        self.crvs = self.create_contours(srf, height)
        self.get_bricks()
        
    def wall_from_brep(self,brep):
        self.crvs = self.create_contours(brep, height)
        self.get_bricks()


class Brick(object):
    
    """
    this is a class to make a brick
    """
    
    global length
    global width
    global height

    # this method gets calles as soon as the object of a class is initiated
    # here we define instance variables = attributes of the object
    def __init__(self, origin, vec):
        
        
        self.origin = origin 
        self.vec = vec # point's tangent vector

        
        self.pts = self.brick_pts()
        
        self.start_plane = self.get_start_plane()
        self.target_plane = self.get_target_plane()
        
        self.mesh = self.brick_mesh()
        self.neighbors= []
        self.color = []
        
        
        
    # this method will be called in the __init__ function
    # it returns a list of 8 points of the bounding box 
    def brick_pts(self):
        
        pt_0 = rg.Point3d(self.origin.X-length/2, self.origin.Y-width/2, self.origin.Z)
        pt_1 = rg.Point3d(pt_0.X + length, pt_0.Y, self.origin.Z)
        pt_2 = rg.Point3d(pt_1.X, pt_1.Y + width, self.origin.Z)
        pt_3 = rg.Point3d(pt_2.X - length, pt_2.Y, self.origin.Z)

        pt_4 = rg.Point3d(pt_0.X, pt_0.Y, pt_0.Z + height)
        pt_5 = rg.Point3d(pt_1.X, pt_1.Y, pt_1.Z + height)
        pt_6 = rg.Point3d(pt_2.X, pt_2.Y, pt_2.Z + height)
        pt_7 = rg.Point3d(pt_3.X, pt_3.Y, pt_3.Z + height)

        b_pts = [pt_0, pt_1, pt_2, pt_3, pt_4, pt_5, pt_6, pt_7]

        return b_pts
    
    # this method is called in the __init__ function
    # it returns the start_plane of the brick accodring to boolean input
    def get_start_plane(self):
        
        yvec = rg.Vector3d.CrossProduct(rg.Vector3d(0,0,1), self.vec)
        start_plane = rg.Plane(self.origin, self.vec, yvec)
            
        return start_plane
    
    # this method is called in the __init__ function
    # it returns the rotated start plabe plane according to input value 
    def get_target_plane(self):
        c = copy.copy(self.start_plane)
        c.Rotate(math.radians(bricks_angle), rg.Vector3d.ZAxis, self.origin)
        return c
    
    # this method is called in the __init__ funciton
    # it creates the mesh from 8 points, transforms its plane and returns the mesh 
    def brick_mesh(self):
        br = rg.Mesh.CreateFromBox(self.pts,1,1,1)
        P = rg.Plane(self.origin, rg.Vector3d.XAxis, rg.Vector3d.YAxis)
        T1 = rg.Transform.PlaneToPlane(P, self.start_plane)
        T2 = rg.Transform.PlaneToPlane(self.start_plane, self.target_plane)
        br.Transform(T1)
        br.Transform(T2)
        
        return br

